// Membuat bilangan ganjil dan bilangan genap
#include <stdio.h>

int main() {
    for(int i=0; i<100; i++){
        printf("%d \n", i);
    
    int Nilai = i % 2;
    
    if (Nilai == 1){
        printf("Bilangan ganjil\n");
    }
    else {
        printf("Bilangan genap\n");
    }
    }
    return 0;
}
